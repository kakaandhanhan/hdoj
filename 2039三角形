为了记录自己的做题
#include <stdio.h>
#define EMS 1e-6
int main()
{
	int n;
	scanf("%d", &n);
	int i;
	for (i = 0; i < n; i++)
	{
		double a, b, c;
		scanf("%lf %lf %lf", &a, &b, &c);
		if (a + b >c&& a + c > b && b + c > a)
		{
			printf("YES\n");
		}
		else
		{
			printf("NO\n");
		}
	}
	return 0;
}
